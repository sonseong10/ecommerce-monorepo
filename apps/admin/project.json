{
  "name": "admin",
  "root": "apps/admin",
  "sourceRoot": "apps/admin/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "react-scripts build"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "react-scripts start",
        "cwd": "apps/admin"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "react-scripts test"
      }
    }
  },
  "tags": []
}
